<table>
  <tr>
    <th>Project Name</th>
    <th>client</th>
    <th>Cost</th>
    <th>Starting Date</th>
    <th>Ending Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
<% @projects.each do |project| %>
  <tr>
    <td><%= project.name %></td>
    <td><%= project.client %></td>
    <td><%= project.cost %></td>
    <td><%= project.starting_date %></td>
    <td><%= project.ending_date %></td>
    <td>

    <%if project.team.nil?%>
      <%= link_to "Assign Team", new_project_team_path(project.id) %>
    <% else  %>
      <%= link_to "Team", project_teams_path(project.id) %>
    <% end %>

    </td>
    <td><%= link_to "Edit", edit_project_path(project.id) %></td>
    <td><%= link_to "Delete", project, :confirm=>"Are You Sure?", :method=> :delete %></td>
  </tr>
<% end %>
</table>
<br />
