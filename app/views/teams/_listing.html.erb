<table>
  <tr>
    <th>Team Members</th>
  </tr>
<% @project.team.people.each do |person| %>
  <tr>
    <td>
    <%= person.name %>
    </td>
    <td><%= link_to "Edit", edit_project_team_path(@project.id,@project.team.id) %></td>
    <td><%= link_to "Delete", project_path(@project.team.project.id), :confirm=>"Are You Sure?", :method=> :delete %></td>
  </tr>
<% end %>
</table>
<br />
